<UserControl x:Class="AppAMI.MeterConfig.MeterConfiguration"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="1270"  >

    <UserControl.Resources>
        <Style x:Key="filterControlStyle" TargetType="syncfusion:GridFilterControl">
            <Setter Property="FilterMode" Value="Both"  />
        </Style>
    </UserControl.Resources>

    
    <Grid>
        <Grid >

            <Grid.RowDefinitions>
                <RowDefinition Height="40"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>


            <Grid Grid.Row="1" Background="#202020">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="200"/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>

                <Grid Background="#2d2d30">
                    <TreeView x:Name="myTreeView"  Margin="0 0 0 15" 
                      SelectedItemChanged="myTreeView_SelectedItemChanged"
                      Background="#2d2d30"   
                          Style="{StaticResource MaterialDesignTreeView}">
                        <TreeView.ItemContainerStyle>
                            <Style TargetType="{x:Type TreeViewItem}" BasedOn="{StaticResource MaterialDesignTreeViewItem}">
                                <Setter Property="Foreground" Value="White" />
                                <Style.Resources>
                                    <Style TargetType="{x:Type TreeViewItem}" BasedOn="{StaticResource MaterialDesignTreeViewItem}">
                                        <Setter Property="Foreground" Value="White"/>
                                    </Style>
                                </Style.Resources>
                            </Style>
                        </TreeView.ItemContainerStyle>
                    </TreeView>
                </Grid>

                <Grid Grid.Column="1">
                    <Grid.RowDefinitions>
                        <RowDefinition x:Name="grdUsersRow"/>
                        <RowDefinition x:Name="grdEventsRow" Height="auto"/>
                    </Grid.RowDefinitions>

                    <Grid     Margin="15">
                        <Rectangle  RadiusY="5" RadiusX="5" Fill="#2d2d30"   >
                            <Rectangle.Effect>
                                <DropShadowEffect BlurRadius="15" Direction="0" RenderingBias="Quality"
                                          ShadowDepth="1" Color="#2d2d30"/>
                            </Rectangle.Effect>
                        </Rectangle>
                        <Grid Margin="15">
                            <syncfusion:SfDataGrid Grid.Row="0" x:Name="datagridDT"  Visibility="Visible"      
                                               HorizontalAlignment="Center" 
                                               AutoGenerateColumns="False"  
                                               
                                               AllowResizingHiddenColumns="True" 
                                               AllowRowHoverHighlighting="True" 
                                               
                                               AllowEditing="False"
                                               AllowDeleting="False"
                                               AllowResizingColumns="True"
                                               FilterPopupStyle="{StaticResource filterControlStyle}"
                                               AllowSorting="True"
                                               AllowFiltering="True"
                                               AllowGrouping="True"
                                               SelectionMode="Single"
                                               NavigationMode="Row"
                                               ShowGroupDropArea="False"  
                                               ItemsSource="{Binding}"
                                               Background="#2d2d30" 
                                               Foreground="White"  
                                               RowHeight="25"    
                                               HeaderRowHeight="30"
                                               PreviewMouseRightButtonDown="datagridDT_PreviewMouseRightButtonDown" >
                                <syncfusion:SfDataGrid.Resources>
                                    <Style TargetType="syncfusion:GridHeaderCellControl">
                                        <Setter Property="Background" Value="#2d2d30" />
                                        <Setter Property="Foreground" Value="White" />
                                        <Setter Property="FontSize" Value="12" />

                                    </Style>


                                </syncfusion:SfDataGrid.Resources>

                                <syncfusion:SfDataGrid.Columns>


                                    <syncfusion:GridTextColumn   MappingName="district_name" >
