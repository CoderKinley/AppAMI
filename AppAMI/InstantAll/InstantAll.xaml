<UserControl x:Class="AppAMI.InstantAll.InstantAll"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:syncfusion="clr-namespace:Syncfusion.UI.Xaml.Grid;assembly=Syncfusion.SfGrid.WPF" xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" xmlns:Syncfusion="clr-namespace:Syncfusion.UI.Xaml.ProgressBar;assembly=Syncfusion.SfProgressBar.WPF"
             mc:Ignorable="d" 
              d:DesignHeight="600" d:DesignWidth="1270">
    
    
    <UserControl.Resources>
        <Style x:Key="filterControlStyle" TargetType="syncfusion:GridFilterControl">
            <Setter Property="FilterMode" Value="Both"  />
        </Style>

        <Style TargetType="Button" x:Key="btnMinNor">
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="Foreground" Value="SlateGray"  />
            <Setter Property="Width"  Value="30" />
            <Setter Property="Height"   Value="30" />
            <Setter Property="BorderThickness"    Value="0" />
            <Setter Property="Margin"     Value="0,0,0,5" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Border Background="{TemplateBinding Background}"
                         CornerRadius="0"
                         BorderThickness="0"
                         Padding="0"
                         BorderBrush="#000">
                            <ContentPresenter HorizontalAlignment="Center"   VerticalAlignment="Center">

                            </ContentPresenter>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>

            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Background" Value="Gray"   />
                    <Setter Property="Foreground" Value="Black" />
                </Trigger>
            </Style.Triggers>
        </Style>
    </UserControl.Resources>
 
    
    <Grid>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="40"/>
                <RowDefinition Height="*"/>
                
            </Grid.RowDefinitions>

            <Grid Background="#2d2d30">

                <Grid.ColumnDefinitions>
                    <ColumnDefinition />

                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="auto"/>

                </Grid.ColumnDefinitions>

                <Button  Grid.Column="0" x:Name="btnDisConnectConnectMqtt"  Click="btnDisConnectConnectMqtt_Click"  Height="30"  
                                    Width="35" BorderBrush="Transparent"    Background="Transparent"   Visibility="Collapsed" HorizontalAlignment="Right"   >

                    <materialDesign:PackIcon Kind="LanConnect"    BorderThickness="0" 
                                                         Foreground="#ff5768" Margin="-5" 
                                                         HorizontalAlignment="Center"     
                                                         VerticalAlignment="Center"  />
                </Button>
                <Button Grid.Column="0" x:Name="btnConnectMqtt" Click="btnConnectMqtt_Click"  BorderThickness="0"  Height="30" Width="35" 
                                     Background="Transparent"  HorizontalAlignment="Right" >
                    <materialDesign:PackIcon Kind="LanConnect"      Foreground="#00cdac" Margin="-5" 
                                                         HorizontalAlignment="Center"    
                                                         VerticalAlignment="Center"  />

                </Button>



                <Grid Grid.Column="1" x:Name="grdAllESDSummary"  >
                    <Button  Grid.Column="0" x:Name="BtnPauseMqttCountry"  Click="BtnPauseMqttCountry_Click" BorderThickness="0" Height="30"  
                                    Width="35" BorderBrush="Transparent"    Background="Transparent"   Visibility="Collapsed"  >

                        <materialDesign:PackIcon Kind="Pause"    BorderThickness="0" 
                                                         Foreground="#ff5768" Margin="-5" 
                                                         HorizontalAlignment="Center"     
                                                         VerticalAlignment="Center"  />
                    </Button>

                    <Button Grid.Column="0" x:Name="BtnPlayMqttCountry" Click="BtnPlayMqttCountry_Click"  BorderThickness="0"  Height="30" Width="35" 
                                    BorderBrush="Transparent" Background="Transparent" >
                        <materialDesign:PackIcon Kind="Play"     Foreground="#00cdac" Margin="-5" 
                                                         HorizontalAlignment="Center"    
                                                         VerticalAlignment="Center"  />

                    </Button>

                </Grid>

                <StackPanel Grid.Column="2" Orientation="Horizontal" HorizontalAlignment="Right"  Margin="0 0 10 0">




                    <Label x:Name="lblConnStatus" Grid.Column="1"  VerticalAlignment="Center"/>
                    <Button x:Name="btnExport"  Style="{StaticResource btnMinNor}" HorizontalAlignment="Right"  Width="50"   PreviewMouseRightButtonDown="btnExport_PreviewMouseRightButtonDown"   >
                        <materialDesign:PackIcon Kind="FileExportOutline"  Foreground="#00a5e3" HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Button>
                </StackPanel>



            </Grid>

            <Grid Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="200"/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>

                <Grid Background="#2d2d30">

                    <Grid.RowDefinitions>
                        <RowDefinition Height="40" />
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <StackPanel Grid.Row  ="0" Orientation="Horizontal"  Margin="10 0 0 0">

                        <CheckBox   x:Name="cbNationData"     Margin="0 10 0 0"  
                                  Height="25" Content="       All ESD Data"
                                             Foreground="#cfcfcf" IsThreeState="False"  
                                      Checked="cbNationData_Checked" Unchecked="cbNationData_Unchecked"  >
                            <CheckBox.Resources>
                                <SolidColorBrush x:Key="MaterialDesignCheckBoxOff" Color="Orange"/>
                                <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="Orange"/>
                            </CheckBox.Resources>
                        </CheckBox>


                    </StackPanel>



                    <TreeView x:Name="myTreeView"  Grid.Row="1" Margin="0 0 0 15"
                      SelectedItemChanged="myTreeView_SelectedItemChanged"
                      Background="#2d2d30"   
                          Style="{StaticResource MaterialDesignTreeView}">

                        <TreeView.ItemContainerStyle>
                            <Style TargetType="{x:Type TreeViewItem}" BasedOn="{StaticResource MaterialDesignTreeViewItem}">
                                <Setter Property="Foreground" Value="White" />

                                <Style.Resources>
                                    <Style TargetType="{x:Type TreeViewItem}" BasedOn="{StaticResource MaterialDesignTreeViewItem}">
                                        <Setter Property="Foreground" Value="White"/>
                                    </Style>
                                </Style.Resources>
                            </Style>
                        </TreeView.ItemContainerStyle>

                    </TreeView>
                </Grid>

                <Grid Grid.Column="1"  Background="#202020" >

                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition x:Name="grdEventsRow" Height="auto"/>
                    </Grid.RowDefinitions>

                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="55"/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>

                        <Grid Margin="15 15 5 5">
                            
                            <StackPanel x:Name="stackCurrentData" Orientation="Horizontal"  />

                            <Grid Grid.Row="1" Background="#00a5e3" Height="1" VerticalAlignment="Bottom" />


                        </Grid>

                        <Grid x:Name="grdHideButton" Background="#202020"  Visibility="Collapsed" >

                        </Grid>
                        
                        <Grid Grid.Row="1" Margin="15 0 15  15" x:Name="grdInstantView">


                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition Height="auto"/>
                            </Grid.RowDefinitions>

                            <Grid Grid.Row="0" Margin="0 0 0 7.5">
                                <Rectangle  RadiusY="5" RadiusX="5" Fill="#2d2d30"   >
                                    <Rectangle.Effect>
                                        <DropShadowEffect BlurRadius="15" Direction="0" RenderingBias="Quality"
                                          ShadowDepth="1" Color="#2d2d30"/>
                                    </Rectangle.Effect>
                                </Rectangle>

                                <Grid Margin="15">
