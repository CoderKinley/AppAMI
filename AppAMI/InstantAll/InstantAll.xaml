                                                        <TextBlock Text="{Binding feeder_id}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                    </DataTemplate>
                                                </syncfusion:GridTextColumn.CellTemplate>
                                            </syncfusion:GridTextColumn>

                                            <syncfusion:GridTextColumn  MappingName="location" >
