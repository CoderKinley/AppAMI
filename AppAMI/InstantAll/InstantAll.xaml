                                                </syncfusion:GridTextColumn.CellStyle>
                                                <syncfusion:GridTextColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding active_energy}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                    </DataTemplate>
                                                </syncfusion:GridTextColumn.CellTemplate>
                                            </syncfusion:GridTextColumn>



                                            <syncfusion:GridTextColumn  MappingName="activeEnergyH1">
                                                <syncfusion:GridTextColumn.HeaderTemplate>
                                                    <DataTemplate>
                                                        <TextBlock TextWrapping="Wrap" Text="Active Energy H1" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                    </DataTemplate>
                                                </syncfusion:GridTextColumn.HeaderTemplate>
                                                <syncfusion:GridTextColumn.CellStyle>
                                                    <Style TargetType="syncfusion:GridCell">
                                                        <Setter Property="HorizontalAlignment" Value="Stretch"  />
                                                        <Setter Property="VerticalAlignment"  Value="Stretch"   />
                                                    </Style>
                                                </syncfusion:GridTextColumn.CellStyle>
                                                <syncfusion:GridTextColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding activeEnergyH1}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                    </DataTemplate>
                                                </syncfusion:GridTextColumn.CellTemplate>
                                            </syncfusion:GridTextColumn>


                                            <syncfusion:GridTextColumn  MappingName="activeEnergyH2">
                                                <syncfusion:GridTextColumn.HeaderTemplate>
                                                    <DataTemplate>
                                                        <TextBlock TextWrapping="Wrap" Text="Active Energy H2" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                    </DataTemplate>
                                                </syncfusion:GridTextColumn.HeaderTemplate>
                                                <syncfusion:GridTextColumn.CellStyle>
                                                    <Style TargetType="syncfusion:GridCell">
                                                        <Setter Property="HorizontalAlignment" Value="Stretch"  />
                                                        <Setter Property="VerticalAlignment"  Value="Stretch"   />
                                                    </Style>
                                                </syncfusion:GridTextColumn.CellStyle>
                                                <syncfusion:GridTextColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding activeEnergyH2}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                    </DataTemplate>
                                                </syncfusion:GridTextColumn.CellTemplate>
                                            </syncfusion:GridTextColumn>

                                            <syncfusion:GridTextColumn  MappingName="firmware_version">
                                                <syncfusion:GridTextColumn.HeaderTemplate>
                                                    <DataTemplate>
                                                        <TextBlock TextWrapping="Wrap" Text="Firmware Version" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                    </DataTemplate>
                                                </syncfusion:GridTextColumn.HeaderTemplate>
                                                <syncfusion:GridTextColumn.CellStyle>
                                                    <Style TargetType="syncfusion:GridCell">
                                                        <Setter Property="HorizontalAlignment" Value="Stretch"  />
                                                        <Setter Property="VerticalAlignment"  Value="Stretch"   />
                                                    </Style>
                                                </syncfusion:GridTextColumn.CellStyle>
                                                <syncfusion:GridTextColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding firmware_version}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                    </DataTemplate>
                                                </syncfusion:GridTextColumn.CellTemplate>
                                            </syncfusion:GridTextColumn>


                                        </syncfusion:SfDataGrid.Columns>
                                    </syncfusion:SfDataGrid>



                                   

                                </Grid>

                                <Grid x:Name="grdEmailAddress" Height="150" Width="350" Background="White"   >
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="30" />
                                        <RowDefinition />
                                        <RowDefinition Height="40"/>
                                    </Grid.RowDefinitions>



                                    <Grid Background="#202020">
                                        <Label Content="Email Address" Foreground="White"   
                                   HorizontalAlignment="Center" 
                                   VerticalAlignment="Center" />
                                    </Grid>

                                    <StackPanel Grid.Row="1" VerticalAlignment="Center" >

                                        <StackPanel      Orientation="Horizontal"   >
                                            <Label Content="Enter Recipient Email Address" 
                  HorizontalAlignment="Left"  
                    Margin=" 20 0"/>


                                        </StackPanel>

                                        <TextBox   x:Name="txtEmailAdress" VerticalContentAlignment="Bottom" 
                     VerticalAlignment="Center" HorizontalContentAlignment="Center"   Margin="20 0" />




                                    </StackPanel>
                                    <StackPanel Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Center"  >

                                        <Button  x:Name="btnSendEmail"    Background="#00cdac" BorderThickness="0" Margin="20 0"  Click="btnSendEmail_Click">
                                            <StackPanel Orientation="Horizontal">
                                                <materialDesign:PackIcon Kind="EmailSend"           HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 10 0" />
                                                <Label Content="Send" Foreground="white" Margin="-5" />

                                            </StackPanel>
                                        </Button>


                                        <Button  x:Name="btnCancel"  Background="#ff5768" BorderThickness="0" Margin="20 0"  Click="btnCancel_Click">
                                            <StackPanel Orientation="Horizontal">
                                                <materialDesign:PackIcon Kind="Cancel"            HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 10 0" />
                                                <Label Content="Cancel" Foreground="white" Margin="-5" />

                                            </StackPanel>
                                        </Button>


                                    </StackPanel>
                                </Grid>

                                <Grid>
                                    <Syncfusion:SfCircularProgressBar 
                x:Name="progressLogin"
                Visibility="Collapsed"             
                ProgressContent="Loading..."
               
                Width="100" 
                Height="100" 
                IsIndeterminate="True"  
                ShowProgressValue="False">
                                        <Syncfusion:SfCircularProgressBar.AnimationEasing>
                                            <BounceEase
                Bounces="20"
                Bounciness="5"
                EasingMode="EaseOut" />
                                        </Syncfusion:SfCircularProgressBar.AnimationEasing>
                                    </Syncfusion:SfCircularProgressBar>
                                </Grid>

                            </Grid>


                            <Grid  Grid.Row="1"   Margin="0 7.5 0 0">
                                <Rectangle   RadiusY="5" RadiusX="5"  Fill="#2d2d30"   >
                                    <Rectangle.Effect>
                                        <DropShadowEffect BlurRadius="15" Direction="0" RenderingBias="Quality" 
                                              ShadowDepth="1" Color="#2d2d30"/>
                                    </Rectangle.Effect>
                                </Rectangle>

                                <Grid Grid.Row="1"  Margin=" 5 0 ">
                                    <Grid.ColumnDefinitions >
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                      
                                    </Grid.ColumnDefinitions>

                                    <Grid Grid.Column="0"  >
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
                                            <Label Content="Valid Data:" Foreground="#cfcfcf" FontSize="10"
                                       VerticalAlignment="Center" HorizontalAlignment="Right" />

                                            <Label x:Name="lblValidDataCount" Content="0" Foreground="#00cdac"
                                       FontWeight="Bold"  VerticalAlignment="Center"
                                       HorizontalAlignment="Left" />

                                        </StackPanel>
                                    </Grid>

                                    <Grid Grid.Column="1" >
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
                                            <Label Content="NC:" Foreground="#cfcfcf" FontSize="10"
                                       VerticalAlignment="Center" HorizontalAlignment="Right" />
                                            <Label Content="0" Foreground="#ff5768" x:Name="lblTotalNCCount" FontWeight="Bold" 
                                       VerticalAlignment="Center" HorizontalAlignment="Left" />

                                        </StackPanel>

                                    </Grid>

                                    <Grid Grid.Column="2" >
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
                                            <Label Content="Total MRI Reporting: " Foreground="#cfcfcf" FontSize="10"
                                       VerticalAlignment="Center" HorizontalAlignment="Right" />
                                            <Label Content="0" Foreground="#00a5e3" x:Name="lblTotalDataCount" FontWeight="Bold" 
                                       VerticalAlignment="Center" HorizontalAlignment="Left" />

                                        </StackPanel>

                                    </Grid>

                                    <Grid Grid.Column="3" >
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
                                            <Label Content="% Valid Data: " Foreground="#cfcfcf" FontSize="10"
                                       VerticalAlignment="Center" HorizontalAlignment="Right" />
                                            <Label Content="0" Foreground="#00a5e3" x:Name="lblPercentReporting" FontWeight="Bold" 
                                       VerticalAlignment="Center" HorizontalAlignment="Left" />

                                        </StackPanel>

                                    </Grid>

                                    <Grid Grid.Column="4"  >
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
                                            <Label Content="Successfull Polls:" Foreground="#cfcfcf" FontSize="10"
                                       VerticalAlignment="Center" HorizontalAlignment="Right" />

                                            <Label x:Name="lblSuccessfulPolls" Content="0" Foreground="#00cdac"
                                       FontWeight="Bold"  VerticalAlignment="Center"
                                       HorizontalAlignment="Left" />

                                        </StackPanel>
                                    </Grid>

                                    <Grid Grid.Column="5" >
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
                                            <Label Content="Unsuccessful Polls: "  FontSize="10" Foreground="#cfcfcf"
                                       VerticalAlignment="Center" HorizontalAlignment="Right" />
                                            <Label Content="0" Foreground="#ff5768" x:Name="lblFailedPolls" FontWeight="Bold" 
                                       VerticalAlignment="Center" HorizontalAlignment="Left" />

                                        </StackPanel>

                                    </Grid>

                                    <Grid Grid.Column="6" >
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
                                            <Label Content="Total Polls: " Foreground="#cfcfcf" FontSize="10"
                                       VerticalAlignment="Center" HorizontalAlignment="Right" />
                                            <Label Content="0" Foreground="#00a5e3" x:Name="lblTotalPolls" FontWeight="Bold" 
                                       VerticalAlignment="Center" HorizontalAlignment="Left" />

                                        </StackPanel>

                                    </Grid>

                                </Grid>
                            </Grid>


                        </Grid>
                    </Grid>





                </Grid>

            </Grid>

        </Grid>

       
    </Grid>

</UserControl>
