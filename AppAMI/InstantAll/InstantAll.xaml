                                                <DataTemplate>
                                                    <TextBlock TextWrapping="Wrap" Text="Topic" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                </DataTemplate>
                                            </syncfusion:GridTextColumn.HeaderTemplate>
                                            <syncfusion:GridTextColumn.CellStyle>
                                                <Style TargetType="syncfusion:GridCell">
                                                    <Setter Property="HorizontalAlignment" Value="Stretch"  />
                                                    <Setter Property="VerticalAlignment"  Value="Stretch"   />
                                                </Style>
                                            </syncfusion:GridTextColumn.CellStyle>
                                            <syncfusion:GridTextColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding instantaneous}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                </DataTemplate>
                                            </syncfusion:GridTextColumn.CellTemplate>
                                        </syncfusion:GridTextColumn>

                                            <syncfusion:GridTextColumn   MappingName="phase" >
                                                <syncfusion:GridTextColumn.HeaderTemplate>
                                                    <DataTemplate>
                                                        <TextBlock TextWrapping="Wrap" Text="Phase" HorizontalAlignment="Center" VerticalAlignment="Center" />
