                                                        <TextBlock Text="{Binding mri_serial_no}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                    </DataTemplate>
                                                </syncfusion:GridTextColumn.CellTemplate>
                                            </syncfusion:GridTextColumn>


                                            <syncfusion:GridTextColumn   MappingName="instantaneous" >
