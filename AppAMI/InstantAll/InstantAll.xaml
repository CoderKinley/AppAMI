                                               AutoGenerateColumns="False"  
                                               
                                               AllowResizingHiddenColumns="True" 
                                               AllowRowHoverHighlighting="True" 
                                               
                                               AllowEditing="False"
                                               AllowDeleting="False"
                                               AllowResizingColumns="True"
                                               AllowSorting="True"
                                               AllowFiltering="True"
                                               AllowGrouping="True"
                                               SelectionMode="Extended" 
                                               NavigationMode="Cell" 
                                               SelectionUnit="Cell" 
                                               ShowGroupDropArea="False"  
                                               ItemsSource="{Binding}"
                                               Background="#2d2d30" 
                                               Foreground="White"  
                                               RowHeight="25"    
                                               HeaderRowHeight="50" >
