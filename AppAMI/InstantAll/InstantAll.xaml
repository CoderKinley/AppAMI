                                                    <DataTemplate>
                                                        <TextBlock Text="{Binding esd_name}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                    </DataTemplate>
                                                </syncfusion:GridTextColumn.CellTemplate>
                                            </syncfusion:GridTextColumn>

